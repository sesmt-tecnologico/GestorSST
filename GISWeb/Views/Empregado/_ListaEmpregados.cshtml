@using GISModel.Entidades
@using GISModel.DTO.AnaliseDeRisco

@model List<VMAnaliseDeRiscoEmpregados>



@{
    ViewBag.Title = "Listagem de Empregados";

    Layout = null;
}


<div class="row">
    <div class="col-xs-12">
        

        @if (Model != null)
        {
            <div>
                <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Empregado</th>
                            <th>CPF</th>
                           
                        </tr>
                    </thead>

                    <tbody>

                        @foreach (VMAnaliseDeRiscoEmpregados iEmp in Model)
                        {                            

                            <tr id="linha-@iEmp.UKEmpregado" >

                                <td>@iEmp.NomeEmpregado</td>
                                <td>@iEmp.CPF</td>                        

                           
                            </tr>

                        }

                    </tbody>
                </table>
            </div>
        }

    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jQ_dataTable")
    <script src="~/Scripts/Custom/Empresa/Listagem.js"></script>
}
